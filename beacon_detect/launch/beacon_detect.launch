<launch>

  <node name="beacondetector" pkg="beacon_detect" type="beacondetect" output="screen">
      <param name="cam_topic" type="str" value="/upper_stereo_raw/right/image_raw" />
      <param name="configuration" type="str" value="configuration.xml" />
      <param name="tag_size_big" value=" 0.5588" />
      <param name="tag_size_small" value=" 0.2667" />	
      <param name="show_result" value="true" />
      <param name="test_mode"	value="false" /> 
      <param name="histeq"	value="false" /> 
      <param name="robot_topic" value="/aero/supervisor/state" />	
  </node>
</launch>
